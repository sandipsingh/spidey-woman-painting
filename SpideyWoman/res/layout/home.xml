<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/barbieimage1" >

  
    
    

     <ImageView
         android:id="@+id/image_feedback"
         android:layout_width="120dp"
         android:layout_height="40dp"
         android:layout_centerHorizontal="true"
         android:layout_centerVertical="true"
         android:src="@drawable/feedback" />

     <ImageView
         android:id="@+id/imageView2"
         android:layout_width="120dp"
         android:layout_height="40dp"
         android:layout_alignLeft="@+id/image_feedback"
         android:layout_below="@+id/image_feedback"
         android:src="@drawable/moreapps" />

     <ImageView
         android:id="@+id/imageView1"
         android:layout_width="120dp"
         android:layout_height="40dp"
         android:layout_above="@+id/image_feedback"
         android:layout_alignLeft="@+id/image_feedback"
         android:src="@drawable/savedimage" />

     <ImageView
         android:id="@+id/image_start"
         android:layout_width="120dp"
         android:layout_height="40dp"
         android:layout_above="@+id/imageView1"
         android:layout_alignLeft="@+id/imageView1"
         android:src="@drawable/start" />

      <RelativeLayout        
        android:id="@+id/relative_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
         >
    </RelativeLayout>      
     
</RelativeLayout>